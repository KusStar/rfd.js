    "pretest:repl": "npm run build",
    "test:repl": "node --import ./index.test.mjs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.2",
    "prettier": "^3.0.0",
    "serve": "^14.2.0",
    "typedoc": "^0.24.8"
  },
  "typedocOptions": {
    "entryPoints": [
      "index.d.ts"
    ],
    "out": "docs/dist",
    "skipErrorChecking": true
  }
