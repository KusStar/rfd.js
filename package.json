{
  "name": "@jcbhmr/rfd",
  "version": "1.0.0",
  "description": "ðŸ“‚ Rust File Dialog bindings for JavaScript",
  "keywords": [
    "nodejs",
    "rust",
    "bindings",
    "nodejs-addon",
    "napi-rs"
  ],
  "homepage": "https://jcbhmr.github.io/rfd/",
  "bugs": "https://github.com/jcbhmr/rfd/issues",
  "repository": "github:jcbhmr/rfd",
  "license": "MIT",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)",
  "type": "commonjs",
  "exports": {
    "node-addons": "./index.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "index.*.node"
  ],
  "scripts": {
    "build:debug": "napi build --platform",
    "build:docs": "napi build --platform && typedoc",
    "prepack": "bash tools/build-if-needed.sh",
    "prepublishOnly": "bash tools/ensure-all-targets.sh",
    "pretest": "cargo fmt && prettier --ignore-path .gitignore -w .",
    "test": "napi build --platform && node --test test/index.test.mjs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.1",
    "execa": "^7.1.1",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "typedoc": "^0.24.8"
  },
  "_napi": {
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc",
        "universal-apple-darwin"
      ]
    }
  },
  "typedocOptions": {
    "entryPoints": [
      "index.d.ts"
    ],
    "out": "docs/dist",
    "skipErrorChecking": true
  }
}
